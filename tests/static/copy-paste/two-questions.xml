<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Untitled Form</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/3760D84C-832E-48D0-80A8-FC206F09A767" uiVersion="1" version="1" name="Untitled Form">
					<text />
					<choice />
				</data>
			</instance>
			<bind nodeset="/data/text" type="xsd:string" />
			<bind nodeset="/data/choice" />
			<itext>
				<translation lang="en" default="">
					<text id="default-label">
						<value>text with image</value>
					</text>
					<text id="choice-label">
						<value>Do you like it?</value>
					</text>
					<text id="choice-true-label">
						<value>Yes</value>
					</text>
					<text id="choice-false-label">
						<value>No</value>
					</text>
				</translation>
				<translation lang="hin">
					<text id="default-label">
						<value>text with image</value>
					</text>
					<text id="choice-label">
						<value>Do you like it?</value>
					</text>
					<text id="choice-true-label">
						<value>Yes</value>
					</text>
					<text id="choice-false-label">
						<value>No</value>
					</text>
				</translation>
			</itext>
		</model>
	</h:head>
	<h:body>
		<input ref="/data/text">
			<label ref="jr:itext('default-label')" />
		</input>
		<select1 ref="/data/choice">
			<label ref="jr:itext('choice-label')" />
			<item>
				<label ref="jr:itext('choice-true-label')" />
				<value>true</value>
			</item>
			<item>
				<label ref="jr:itext('choice-false-label')" />
				<value>false</value>
			</item>
		</select1>
	</h:body>
</h:html>
