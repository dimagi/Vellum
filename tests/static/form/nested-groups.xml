<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Untitled Form</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/2F1F2BF5-6244-4C20-9B6F-2EF2A7BD2F34" uiVersion="1" version="1" name="Untitled Form">
					<group1>
						<group2>
							<group3 />
						</group2>
					</group1>
				</data>
			</instance>
			<bind nodeset="/data/group1" />
			<bind nodeset="/data/group1/group2" />
			<itext>
				<translation lang="en" default="">
					<text id="group1-label">
						<value>group1</value>
					</text>
					<text id="group1/group2-label">
						<value>group2</value>
					</text>
					<text id="group1/group2/group3-label">
						<value>group3</value>
					</text>
				</translation>
			</itext>
		</model>
	</h:head>
	<h:body>
		<group ref="/data/group1">
			<label ref="jr:itext('group1-label')" />
			<group ref="/data/group1/group2">
				<label ref="jr:itext('group1/group2-label')" />
				<group ref="/data/group1/group2/group3">
					<label ref="jr:itext('group1/group2/group3-label')" />
				</group>
			</group>
		</group>
	</h:body>
</h:html>
